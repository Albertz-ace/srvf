
  <div class="content-wrapper">
      <div class="card">

        <div id="edit-profile" v-cloak>
          <div class="container pt-5 pb-5">
            <h1>Update personal info</h1>
            <hr/>

        
            <ajax-form action="updateProfile" :syncing.sync="syncing" :cloud-error.sync="cloudError" :form-data="formData" :form-rules="formRules" :form-errors.sync="formErrors" @submitted="submittedForm()">
              <div class="row">
                <div class="col-sm-6">
                  <div class="form-group">
                    <label for="full-name">First Name</label>
                    <input class="form-control" id="full-name" name="full-name" type="text"  :class="[formErrors.FirstName ? 'is-invalid' : '']" v-model.trim="formData.FirstName" placeholder="Sturgis P. Sturgeon" autocomplete="name" focus-first>
                    <div class="invalid-feedback" v-if="formErrors.FirstName">Please enter your full name.</div>
                  </div>
                  <div class="form-group">
                    <label for="full-name">Church Organization</label>
                    <input class="form-control" id="Church_Organization" name="Church_Organization" type="text"  :class="[formErrors.Church_Organization ? 'is-invalid' : '']" v-model.trim="formData.Church_Organization" placeholder="Seventh Day" autocomplete="name" >
                    <div class="invalid-feedback" v-if="formErrors.Church_Organization">Please enter your church Organization.</div>
                  </div>
                  <div class="form-group">
                    <label for="full-name">Phone Number</label>
                    <input class="form-control" id="Phone_Number" name="Phone_Number" type="text"  :class="[formErrors.Phone_Number ? 'is-invalid' : '']" v-model.trim="formData.Phone_Number" placeholder="791084993" autocomplete="name" >
                    <div class="invalid-feedback" v-if="formErrors.Phone_Number">Please enter your Phone Number.</div>
                  </div>
                  <div class="form-group">
                    <label for="full-name">CFMS Mobile Pin</label>
                    <input class="form-control" id="CFMS_Mobile_Pin" name="CFMS_Mobile_Pin" type="text"  :class="[formErrors.CFMS_Mobile_Pin ? 'is-invalid' : '']" v-model.trim="formData.CFMS_Mobile_Pin" placeholder="1234" autocomplete="name" >
                    <div class="invalid-feedback" v-if="formErrors.CFMS_Mobile_Pin">Please enter your CFMS Pin</div>
                  </div>
                </div>

                <div class="col-sm-6">
                  <div class="form-group">
                    <label for="email-address">Email address</label>
                    <input class="form-control" id="email-address" name="email-address" type="email"  :class="[formErrors.emailAddress ? 'is-invalid' : '']" v-model.trim="formData.emailAddress" placeholder="sturgeon@example.com" autocomplete="email">
                    <div class="invalid-feedback" v-if="formErrors.emailAddress">Please enter a valid email address.</div>
                  </div>

                  <div class="form-group">
                    <label for="Personel">Personel</label>

                    <div class="input-group mb-3" id="Personel" name="Personel" >
                      <select class="form-control" id="Login_Level"  name="Login_Level" ><option selected>Personel...</option>
                      <option value="President & EXC. Sec">President & EXC. Sec</option>
                       <option value="Treasurer  & Asst. Treasurer ">Treasurer  & Asst. Treasurer </option>
                       <option value="Snr. Accountant">Snr. Accountant</option>
                       <option value="IT Mnager, Asst. Manager & CFMS Coordinator">IT Mnager, Asst. Manager & CFMS Coordinator</option>
                       <option value="Internal Auditer &GCAS auditors">Internal Auditer &GCAS auditors</option>
                       <option value="Trust Fund Accountants">Trust Fund Accountants</option>
                       <option value="Treassure & Asst. Treassure">Treassure & Asst. Treassure</option>
                       <option value="District Pastor">District Pastor</option>
                       <option value="First Elder">First Elder</option>
                       <option value="Deacons & Deaconesses">Deacons & Deaconesses</option>
                       <option value="Treasurer & Treasury clercks">Treasurer & Treasury clercks</option>
                       <option value="Church Clerck">Church Clerck</option>
                       <option value="IT Admin">IT Admin</option>
                       <option value="HoD">HoD</option>
                       <option value="Church Members">Church Members</option>
                      </select> 
                    </div>
                    
                
                  </div>

                  <div class="form-group">
                    <label for="email-address">User Group</label>
                    <input class="form-control" id="User_Group" name="User_Group" type="text"  :class="[formErrors.User_Group ? 'is-invalid' : '']" v-model.trim="formData.User_Group" placeholder="Executive" autocomplete="name">
                    <div class="invalid-feedback" v-if="formErrors.User_Group"> Please enter your User Group</div>
                  </div>
                  <div class="form-group">
                    <label for="email-address">Organization level</label>
                    <input class="form-control" id="Organization_level" name="Organization_level" type="text"  :class="[formErrors.Organization_level ? 'is-invalid' : '']" v-model.trim="formData.Organization_level" placeholder="Organization level" autocomplete="name">
                    <div class="invalid-feedback" v-if="formErrors.Organization_level"> Please enter your Organization Level</div>
                  </div>

                </div>
              </div>



              <div class="row">
                <div class="col-sm-6">
                  <cloud-error v-if="cloudError === 'emailAlreadyInUse'">There is already an account using that email address.</cloud-error>
                  <cloud-error v-if="cloudError"></cloud-error>
                </div>
                <div class="col-sm-6">
                  <div class="form-group text-right">
                    <a class="btn btn-outline-info" href="/account">Cancel</a>
                    <ajax-button type="submit" :syncing="syncing" class="btn btn-dark">Save changes</ajax-button>
                  </div>
                </div>
              </div>
            </ajax-form>

          </div>
        </div>

      </div>
    </div>
     <!-- jQuery -->
 <script src="../../plugins/jquery/jquery.min.js"></script>
 <!-- Bootstrap 4 -->
 <script src="../../plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
 <!-- AdminLTE App -->
 <script src="../../dist/js/adminlte.min.js"></script>

<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>
