<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>CFMS</title>
  <link rel="stylesheet" href="../../../dist/css/adminlte.min.css">

  <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<!------ Include the above in your HEAD tag ---------->

<link href="//netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//netdna.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-2.1.3.min.js"></script>
<!------ Include the above in your HEAD tag ---------->

</head>
<body class="hold-transition sidebar-mini">
<div class="wrapper">
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">             
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/organizationlevel">organizationlevel</a></li>
              <li class="breadcrumb-item active">Add</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>
    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">
            
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">Create New Organization Level</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                <div id="account-overview" v-cloak>  

                  <ajax-form class="well form-horizontal" method= "post" action="addOrganizationlevel" :form-data="formData" >
                     
                    <div class="col-sm-6">
                    <fieldset>

                          <div class="form-group"> 
                            <label class="col-md-4 control-label">Hierachy Level</label>
                              <div class="col-md-4 selectContainer">
                              <div class="input-group">
                                  <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                                  <select class="form-control selectpicker" id="Hierachy_level" name="Hierachy_level"  type="text"  :class="[formErrors.Hierachy_level ? 'is-invalid' : '']" v-model.trim="formData.Hierachy_level"  autocomplete="name" >
                                    <option value="None"selected="selected">Default</option>
                                    <%orghierachlevelData.forEach(function(organization){%>
                                      <option value="<%= organization.Org_description %>"> <%= organization.Org_description %></option>
                                    <% }) %>
                                    
                                  </select>
                              
                            </div>
                          </div>
                          </div>  

                          <div class="form-group"> 
                            <label class="col-md-4 control-label">Parent level</label>
                              <div class="col-md-4 selectContainer">
                              <div class="input-group">
                                  <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                                  <select class="form-control selectpicker" id="Parent_level" name="Parent_level"  type="text"  :class="[formErrors.Parent_level ? 'is-invalid' : '']" v-model.trim="formData.Parent_level"  autocomplete="name" >
                                    <option value="None"selected="selected">Default</option>
                                    <%organizationlevelData.forEach(function(organizationlevel){%>
                                      <option value="<%= organizationlevel.Level_Name %>"> <%= organizationlevel.Level_Name %></option>
                                    <% }) %>
                                    
                                  </select>
                              
                            </div>
                          </div>
                          </div> 
                          
                          <!-- Text input-->                
                          <div class="form-group">
                            <label class="col-md-4 control-label" >Level Name</label> 
                              <div class="col-md-4 inputGroupContainer">
                              <div class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                            <input class="form-control" id="Level_Name" type="text"  :class="[formErrors.Level_Name ? 'is-invalid' : '']" v-model.trim="formData.Level_Name" placeholder="Doe" autocomplete="name" >
                            <div class="invalid-feedback" v-if="formErrors.Level_Name">Please enter Level Name</div>
                          </div>
                            </div>
                          </div>
                             <!-- Text input-->                
                          <div class="form-group">
                              <label class="col-md-4 control-label" >Continent</label> 
                                <div class="col-md-4 inputGroupContainer">
                                <div class="input-group">
                              <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                              <input class="form-control" id="Continent" type="text"  :class="[formErrors.Continent ? 'is-invalid' : '']" v-model.trim="formData.Continent" placeholder="Africa" autocomplete="name" >
                              <div class="invalid-feedback" v-if="formErrors.Continent">Please enter Continent</div>
                            </div>
                              </div>
                          </div>
                               <!-- Text input-->                
                          <div class="form-group">
                            <label class="col-md-4 control-label" >Region</label> 
                              <div class="col-md-4 inputGroupContainer">
                              <div class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                            <input class="form-control" id="Region" type="text"  :class="[formErrors.Region ? 'is-invalid' : '']" v-model.trim="formData.Region" placeholder="Region" autocomplete="name" >
                            <div class="invalid-feedback" v-if="formErrors.Region">Please enter Level Name</div>
                          </div>
                            </div>
                          </div>
                             <!-- Text input-->                
                          <div class="form-group">
                            <label class="col-md-4 control-label" >Country</label> 
                              <div class="col-md-4 inputGroupContainer">
                              <div class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                            <input class="form-control" id="Country" type="text"  :class="[formErrors.Country ? 'is-invalid' : '']" v-model.trim="formData.Country" placeholder="Kenya" autocomplete="name" >
                            <div class="invalid-feedback" v-if="formErrors.Country">Please enter Country</div>
                          </div>
                            </div>
                          </div>
                            <!-- Text input-->                
                          <div class="form-group">
                            <label class="col-md-4 control-label" >State</label> 
                              <div class="col-md-4 inputGroupContainer">
                              <div class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                            <input class="form-control" id="State" type="text"  :class="[formErrors.State ? 'is-invalid' : '']" v-model.trim="formData.State" placeholder="State" autocomplete="name" >
                            <div class="invalid-feedback" v-if="formErrors.State">Please enter State</div>
                          </div>
                            </div>
                          </div>
                    </fieldset>
                    </div>                             

                  <div class=" col-md-6">
                    <fieldset>
                      <!-- Text input-->                
                      <div class="form-group">
                          <label class="col-md-4 control-label" >County</label> 
                            <div class="col-md-4 inputGroupContainer">
                            <div class="input-group">
                          <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                          <input class="form-control" id="County" type="text"  :class="[formErrors.County ? 'is-invalid' : '']" v-model.trim="formData.County" placeholder="Nairobi" autocomplete="name" >
                          <div class="invalid-feedback" v-if="formErrors.County">Please enter County</div>
                        </div>
                          </div>
                      </div>
                      
                      <!-- Text input-->                  
                      <div class="form-group">
                            <label class="col-md-4 control-label">Phone No.</label>  
                              <div class="col-md-4 inputGroupContainer">
                              <div class="input-group">
                                  <span class="input-group-addon"><i class="glyphicon glyphicon-earphone"></i></span>
                                  <input class="form-control" id="Telephone" type="text"  :class="[formErrors.Telephone ? 'is-invalid' : '']" v-model.trim="formData.Telephone" placeholder="0123456789" autocomplete="name" >
                                  <div class="invalid-feedback" v-if="formErrors.Telephone">Please Enter Phone Number</div>
                                </div>
                            </div>
                      </div>
              
                      <!-- Text input-->
                      <div class="form-group">
                            <label class="col-md-4 control-label">E-Mail</label>  
                              <div class="col-md-4 inputGroupContainer">
                              <div class="input-group">
                                  <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
                                  <input class="form-control" id="Email_Address" type="email"  :class="[formErrors.Email_Address ? 'is-invalid' : '']" v-model.trim="formData.Email_Address" placeholder="sturgeon@example.com" autocomplete="email">
                              <div class="invalid-feedback" v-if="formErrors.Email_Address">Please enter a valid email address.</div>
                            </div>
                            </div>
                      </div>
                      <!-- Text input-->
                      <div class="form-group">
                        <label class="col-md-4 control-label">Postal Address</label>  
                          <div class="col-md-4 inputGroupContainer">
                          <div class="input-group">
                              <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
                              <input class="form-control" id="Postal_Address" type="text"  :class="[formErrors.Postal_Address ? 'is-invalid' : '']" v-model.trim="formData.Postal_Address" placeholder="P.O.BOX 444" autocomplete="Postal_Address">
                          <div class="invalid-feedback" v-if="formErrors.Postal_Address">Please enter Postal Address.</div>
                        </div>
                        </div>
                      </div>
                      <!-- Text input-->                
                      <div class="form-group">
                        <label class="col-md-4 control-label" >Currency</label> 
                          <div class="col-md-4 inputGroupContainer">
                          <div class="input-group">
                        <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                        <input class="form-control" id="Currency" type="text"  :class="[formErrors.Currency ? 'is-invalid' : '']" v-model.trim="formData.Currency" placeholder="KSH" autocomplete="name" >
                        <div class="invalid-feedback" v-if="formErrors.Currency">Please enter Currency</div>
                      </div>
                        </div>
                      </div>
                      <!-- Text input-->                
                      <div class="form-group">
                          <label class="col-md-4 control-label" >GL Account Code</label> 
                            <div class="col-md-4 inputGroupContainer">
                            <div class="input-group">
                          <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                          <input class="form-control" id="GL_Account_Code" type="text"  :class="[formErrors.GL_Account_Code ? 'is-invalid' : '']" v-model.trim="formData.GL_Account_Code" placeholder="A1231AC" autocomplete="name" >
                          <div class="invalid-feedback" v-if="formErrors.GL_Account_Code">Please enter GL Account Code</div>
                        </div>
                          </div>
                      </div>
                      <!-- Text input-->
                      <div class="form-group">
                        <label class="col-md-4 control-label">Transaction Code</label>  
                          <div class="col-md-4 inputGroupContainer">
                          <div class="input-group">
                              <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                              <input class="form-control" id="Transaction_Code" type="text"  :class="[formErrors.Transaction_Code ? 'is-invalid' : '']" v-model.trim="formData.Transaction_Code" placeholder="A1231AC" autocomplete="email">
                          <div class="invalid-feedback" v-if="formErrors.Transaction_Code">Please enter Transaction.</div>
                        </div>
                        </div>
                      </div>   
                      
                      

                    </fieldset>
                  </div>
                  <div class="form-group">
                        
                    <label class="col-md-4 control-label"></label>
                    <div class=" col-md-6"><br>
                      <a class="btn btn-outline-info" href="/organizationlevel">&nbsp&nbspCancel&nbsp&nbsp&nbsp&nbsp</a>
                      &nbsp&nbsp&nbsp&nbsp&nbsp<ajax-button type="submit" class="btn btn-warning" :syncing="syncing">&nbsp&nbspSUBMIT <span class="glyphicon glyphicon-send"></span>&nbsp&nbsp&nbsp&nbsp&nbsp</ajax-button>
                    </div>
                  </div>  
         
                  </ajax-form> 

                </div>                
              </div>


              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
</div>
<!-- ./wrapper -->

<!-- jQuery -->
<script src="../../../plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap 4 -->
<script src="../../../plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
