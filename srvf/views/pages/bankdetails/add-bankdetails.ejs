<link href="//netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//netdna.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-2.1.3.min.js"></script>
<!------ Include the above in your HEAD tag ---------->

</head>
<body class="hold-transition sidebar-mini">
<div class="wrapper">
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
             
          </div>

          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/bankdetails">bankdetails</a></li>
              <li class="breadcrumb-item active">Add</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>
    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">

            
            
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">Create New Bank Details</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                <div id="account-overview" v-cloak>   
                               
                          <ajax-form class="well form-horizontal"  method= "post" action="addBankdetails" :form-data="formData" >
                              
                            <fieldset>                                     
                              
          
                              <div class="form-group">
                                <label class="col-md-4 control-label">Bank Name</label>  
                                <div class="col-md-4 inputGroupContainer">
                                <div class="input-group">
                                <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                <input class="form-control" id="Bank_Name" type="text"  :class="[formErrors.Bank_Name ? 'is-invalid' : '']" v-model.trim="formData.Bank_Name" placeholder="Equity " autocomplete="name" >
                                <div class="invalid-feedback" v-if="formErrors.Bank_Name">Please enter Hierachlevel level.</div>
                              </div>
                                </div>
                              </div>    
                      
                              <div class="form-group">
                                <label class="col-md-4 control-label" >Branch Name</label> 
                                  <div class="col-md-4 inputGroupContainer">
                                  <div class="input-group">
                                <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                <input class="form-control" id="Branch" type="text"  :class="[formErrors.Branch ? 'is-invalid' : '']" v-model.trim="formData.Branch" placeholder="Moi Avenue" autocomplete="name" >
                                <div class="invalid-feedback" v-if="formErrors.Branch">Please enter Branch Name</div>
                              </div>
                              </div>
                              </div> 
                              
                              <div class="form-group">
                                <label class="col-md-4 control-label" >Bank Code </label> 
                                  <div class="col-md-4 inputGroupContainer">
                                  <div class="input-group">
                                <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                <input class="form-control" id="Bank_Code" type="text"  :class="[formErrors.Bank_Code ? 'is-invalid' : '']" v-model.trim="formData.Bank_Code" placeholder="0001000203" autocomplete="name" >
                                <div class="invalid-feedback" v-if="formErrors.Bank_Code">Please enter Bank Code</div>
                              </div>
                              </div>
                              </div>

                              <div class="form-group">
                                <label class="col-md-4 control-label" >Account Number </label> 
                                  <div class="col-md-4 inputGroupContainer">
                                  <div class="input-group">
                                <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                <input class="form-control" id="Account_Number" type="text"  :class="[formErrors.Account_Number ? 'is-invalid' : '']" v-model.trim="formData.Account_Number" placeholder="00011112222333444" autocomplete="name" >
                                <div class="invalid-feedback" v-if="formErrors.Account_Number">Please enter Account Number</div>
                              </div>
                              </div>
                              </div> 
                               <!-- Text input--> 
                               
                               <div class="form-group">
                                <label class="col-md-4 control-label"></label>
                                <div class="col-md-4"><br>
                                  <a class="btn btn-outline-info" href="/bankdetails">&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbspCancel&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp</a>
                                  &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp<ajax-button type="submit" class="btn btn-warning" :syncing="syncing">&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbspSUBMIT <span class="glyphicon glyphicon-send"></span>&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp</ajax-button>
                                </div>
                              </div> 
          
                   
                          </fieldset>  

                            
                            
                            </ajax-form>
                          
                     
                  
               </div>
                
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
</div>
<!-- ./wrapper -->

<!-- jQuery -->
<script src="../../../plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap 4 -->
<script src="../../../plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
